<h1>Add Student</h1>


<% if @students.count > 0 %>
    <h3>Click student to enroll</h3>
    <ul>
        <% @students.each do |s| %>
            <li><%= link_to s.name, enroll_path(s), method: :patch %></li>
        <% end %>
    </ul>
<% else %>
    <h3>There are no available students to enroll</h3>
<% end %>
